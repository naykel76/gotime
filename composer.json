{
    "name": "naykel/gotime",
    "description": "Starter package for NayKel Laravel applications",
    "license": "MIT",
    "authors": [
        {
            "name": "Nathan Watts",
            "email": "nathan@naykel.com.au",
            "homepage": "https://naykel.com.au"
        }
    ],
    "homepage": "https://naykel.com.au",
    "keywords": [
        "Laravel",
        "Naykel"
    ],
    "require": {
        "php": "^8.1",
        "livewire/livewire": "^2.10.3",
        "parsedown/laravel": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Naykel\\Gotime\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naykel\\Gotime\\GotimeServiceProvider"
            ],
            "aliases": {
                "Filesys": "Naykel\\Gotime\\Facades\\Filesys",
                "Routebuilder": "Naykel\\Gotime\\Facades\\Routebuilder"
            }
        }
    }
}
